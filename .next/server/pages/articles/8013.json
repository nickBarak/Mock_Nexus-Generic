e=\"1\" data-spotx_unmute_on_mouse=\"1\"'\r\n\r\n];\r\n\r\nif (bMobile) \r\n\r\n{ \r\n\r\naScriptAttributes.push('data-spotx_content_width=\"304\"'); \r\n\r\naScriptAttributes.push('data-spotx_content_height=\"171\"');\r\n\r\naScriptAttributes.push('data-spotx_channel_id=\"128967\"');\r\n\r\n} \r\n\r\nelse \r\n\r\n{ \r\n\r\naScriptAttributes.push('data-spotx_content_width=\"608\"'); \r\n\r\naScriptAttributes.push('data-spotx_content_height=\"342\"'); \r\n\r\naScriptAttributes.push('data-spotx_unmute_on_mouse=\"1\"');\r\n\r\naScriptAttributes.push('data-spotx_channel_id=\"108869\"');\r\n\r\n} \r\n\r\ndocument.write('<scr' + 'ipt ' + aScriptAttributes.join(\" \") + '></scr' + 'ipt>'); \r\n</script> \n<p><span style=\"font-weight: 400;\">Bowl </span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">Cupcake Tray </span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">Baking Cases </span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">Spoon </span></p>\n<p><span style=\"font-weight: 400;\">Ingredients:</span></p>\n<p><span style=\"font-weight: 400;\">100g (3.5 oz.) Rice Krispies cereal (or your favorite cereal)</span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">100g dark, milk and white Chocolate</span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">1.5 Tbsp. agave syrup or honey </span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">30g (1 oz.) butter</span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">5 oz. pretzels  </span></p>\n<p><span style=\"font-weight: 400;\">Directions:</span></p>\n<ol>\n<li><span style=\"font-weight: 400;\"> Break up dark chocolate and melt in the microwave for 45 seconds. </span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">2. Add 1/3 of the agave syrup and butter.</span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">3. Crumble in 1/3 of the Rice Krispies cereal and pretzels. </span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">4. Thoroughly mix and spoon evenly into baking cases.</span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">5. Repeat steps 1-4 with the milk chocolate, then with the white chocolate.*</span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">6. Decorate the tops with a pretzel (not required).</span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">7. Cool in a fridge for 2 hours or in freezer for 30-45 minutes.</span><span style=\"font-weight: 400;\"><br>\n</span><span style=\"font-weight: 400;\">8. Eat and Enjoy!</span></li>\n</ol>\n<p><span style=\"font-weight: 400;\">*Be sure to wash the bowl so you don’t contaminate your white chocolate. You also won’t need as much butter and sugar with the white chocolate because it’s already really sweet. </span></p><div class=\"h5ab-print-button-container\"><div class=\"h5ab-print-button h5ab-print-button-left\" style=\"cursor: pointer; color: #555\"><i class=\"fa fa-print fa-lg\"></i>\n\t\t\t\t\t\t\t<span>Print</span></div></div>\n<div class=\"sharedaddy sd-sharing-enabled\"><div class=\"robots-nocontent sd-block sd-social sd-social-icon sd-sharing\"><h3 class=\"sd-title\">Share this:</h3><div class=\"sd-content\"><ul><li class=\"share-facebook\"><a rel=\"nofollow noopener noreferrer\" data-shared=\"sharing-facebook-70347\" class=\"share-facebook sd-button share-icon no-text\" href=\"https://dailynexus.com/2016-04-25/chocolate-anytime-nests/?share=facebook\" target=\"_blank\" title=\"Click to share on Facebook\"><span></span><span class=\"sharing-screen-reader-text\">Click to share on Facebook (Opens in new window)</span></a></li><li class=\"share-twitter\"><a rel=\"nofollow noopener noreferrer\" data-shared=\"shari