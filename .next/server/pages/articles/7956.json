\"incontent\" data-spotx_autoplay=\"1\" data-spotx_ad_volume=\"0\" data-spotx_loop=\"0\" data-spotx_collapse=\"1\" data-spotx_unmute_on_mouse=\"1\"'\r\n\r\n];\r\n\r\nif (bMobile) \r\n\r\n{ \r\n\r\naScriptAttributes.push('data-spotx_content_width=\"304\"'); \r\n\r\naScriptAttributes.push('data-spotx_content_height=\"171\"');\r\n\r\naScriptAttributes.push('data-spotx_channel_id=\"128967\"');\r\n\r\n} \r\n\r\nelse \r\n\r\n{ \r\n\r\naScriptAttributes.push('data-spotx_content_width=\"608\"'); \r\n\r\naScriptAttributes.push('data-spotx_content_height=\"342\"'); \r\n\r\naScriptAttributes.push('data-spotx_unmute_on_mouse=\"1\"');\r\n\r\naScriptAttributes.push('data-spotx_channel_id=\"108869\"');\r\n\r\n} \r\n\r\ndocument.write('<scr' + 'ipt ' + aScriptAttributes.join(\" \") + '></scr' + 'ipt>'); \r\n</script> \n<p><span style=\"font-weight: 400;\">Ingredients: </span></p>\n<ul>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">1 box of Jiffy cornbread mix</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">2 eggs</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">1 ½ cups of sharp cheddar cheese, shredded</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">1 can of whole-kernel corn</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">2-3 jalape</span><span style=\"font-weight: 400;\">ños</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">1  large onion, chopped</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">3 to 4 strips of bacon, cut into small squares</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Vegetable oil</span></li>\n</ul>\n<p> </p>\n<p><span style=\"font-weight: 400;\">Directions: </span></p>\n<ol>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Preheat the oven to 350 degrees Fahrenheit.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Add some vegetable oil to a pan and saute the onion for about three minutes.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Remove the onion and cook the bacon until crispy.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Mix all of the ingredients together in a large bowl.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Pour the mixture into a glass or metal pan.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Cook for about 35 minutes.</span></li>\n</ol>\n<div id=\"attachment_91447\" style=\"width: 710px\" class=\"wp-caption aligncenter\"><img aria-describedby=\"caption-attachment-91447\" data-attachment-id=\"91447\" data-permalink=\"https://dailynexus.com/2019-11-25/a-mexican-american-thanksgiving/web_fotoreceta/\" data-orig-file=\"https://i2.wp.com/dailynexus.com/wp-content/uploads/2019/11/Web_fotoreceta.jpg?fit=700%2C400&amp;ssl=1\" data-orig-size=\"700,400\" data-comments-opened=\"1\" data-image-meta='{\"aperture\":\"0\",\"credit\":\"\",\"camera\":\"\",\"caption\":\"\",\"created_timestamp\":\"0\",\"copyright\":\"\",\"focal_length\":\"0\",\"iso\":\"0\",\"shutter_speed\":\"0\",\"title\":\"\",\"orientation\":\"0\"}' data-image-title=\"Web_fotoreceta\" data-image-description=\"\" data-medium-file=\"https://i2.wp.com/dailynexus.com/wp-content/uploads/2019/11/Web_fotoreceta.jpg?fit=250%2C143&amp;ssl=1\" data-large-file=\"https://i2.wp.com/dailynexus.com/wp-content/uploads/2019/11/Web_fotoreceta.jpg?fit=700%2C400&amp;ssl=1\" class=\"size-full wp-image-91447\" src=\"https://i2.wp.com/dailynexus.com/wp-content/uploads/2019/11/Web_fotoreceta.jpg?resize=700%2C400\" alt=\"\" width=\"700\" height=\"400\" srcset=\"https://i2.wp.com/dailynexus.com/wp-content/uploads/2019/11/Web_fotoreceta.jpg?w=700&amp;ssl=1 700w, https://i2.wp.com/dailynexus.com/wp-content/uploads/2019/11/Web_fotoreceta.jpg?resize=250%2C143&amp;ssl=1 250w\" sizes=\"(max-width: 700px) 100vw, 700px\" data-recalc-dims=\"1\"><p id=\"caption-attachment-91447\" class=\"wp-caption-text\"><em>Photo Courtesy of superama.com</em></p></div>\n<p><b>Ensalada de manzanas (apple salad) </b></p>\n<p><span style=\"font-weight: 400;\">Ingredients:</span></p>\n<ul>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">5 green apples, peeled and then diced into cubes</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">1 can of pineapple chunks, drained</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">1 cup of raisins </span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">1 cup of pecans chopped </span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">16 ounces of sour cream</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">1 can of lechera (sweetened condensed milk)</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Coconut flakes (optional)</span></li>\n</ul>\n<p> </p>\n<p><span style=\"font-weight: 400;\">Directions</span></p>\n<ol>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">In a large bowl combine all of the ingredients.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Enjoy your super easy and delicious dessert! </span></li>\n</ol>\n<div class=\"h5ab-print-button-container\"><div class=\"h5ab-print-button h5ab-print-button-left\" style=\"cursor: pointer; color: #555\"><i class=\"fa fa-print fa-lg\"></i>\n\t\t\t\t\t\t\t<span>Print</span></div></div>\n<div class=\"saboxplugin-wrap\" itemtype=\"http://schema.org/Person\" itemscope itemprop=\"author\"><div class=\"saboxplugin-gravatar\"><img alt=\"\" src=\"https://secure.gravatar.com/avatar/eff9771f991113650d9761d1e81fce88?s=100&amp;d=mm&amp;r=g\" srcset=\"https://secure.gravatar.com/avatar/eff9771f991113650d9761d1e81fce88?s=200&amp;d=mm&amp;r=g 2x\" clas