=\"myAdDoneFunction\" data-spotx_ad_unit=\"incontent\" data-spotx_autoplay=\"1\" data-spotx_ad_volume=\"0\" data-spotx_loop=\"0\" data-spotx_collapse=\"1\" data-spotx_unmute_on_mouse=\"1\"'\r\n\r\n];\r\n\r\nif (bMobile) \r\n\r\n{ \r\n\r\naScriptAttributes.push('data-spotx_content_width=\"304\"'); \r\n\r\naScriptAttributes.push('data-spotx_content_height=\"171\"');\r\n\r\naScriptAttributes.push('data-spotx_channel_id=\"128967\"');\r\n\r\n} \r\n\r\nelse \r\n\r\n{ \r\n\r\naScriptAttributes.push('data-spotx_content_width=\"608\"'); \r\n\r\naScriptAttributes.push('data-spotx_content_height=\"342\"'); \r\n\r\naScriptAttributes.push('data-spotx_unmute_on_mouse=\"1\"');\r\n\r\naScriptAttributes.push('data-spotx_channel_id=\"108869\"');\r\n\r\n} \r\n\r\ndocument.write('<scr' + 'ipt ' + aScriptAttributes.join(\" \") + '></scr' + 'ipt>'); \r\n</script> \n<p><span style=\"font-weight: 400;\">1 cup of ripe mashed bananas(2-3 bananas, depending on the size)</span></p>\n<p><span style=\"font-weight: 400;\">3 eggs</span></p>\n<p><span style=\"font-weight: 400;\">¼  cup of almond flour</span></p>\n<p><span style=\"font-weight: 400;\">¼  cup of coconut flour</span></p>\n<p><span style=\"font-weight: 400;\">4 oz of chocolate, melted</span></p>\n<p><span style=\"font-weight: 400;\">¼ cup of maple syrup or honey</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of vanilla extract</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of baking powder</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of baking soda</span></p>\n<p><span style=\"font-weight: 400;\">½ cup of your choice of nut butter</span></p>\n<p><span style=\"font-weight: 400;\">Optional toppings: chopped walnuts, bananas and/or chocolate shavings</span></p>\n<p> </p>\n<p>Directions:</p>\n<ol>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Preheat your oven to 350 degrees and prepare a loaf pan with parchment paper. (Optional: I like to spray my parchment paper with cooking spray!)</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Mix your bananas, eggs, maple syrup or honey and nut butter in a bowl.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">In a separate bowl, combine your almond flour, coconut flour, baking powder and baking soda. Mix well.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Combine the two mixtures and then slowly fold in the melted chocolate.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Pour the batter into your prepared loaf pan and add any additional toppings.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Bake for 40-45 minutes and enjoy!</span></li>\n</ol>\n<div id=\"attachment_86700\" style=\"width: 778px\" class=\"wp-caption aligncenter\"><img aria-describedby=\"caption-attachment-86700\" data-attachment-id=\"86700\" data-permalink=\"https://dailynexus.com/2019-03-07/being-gluten-free-in-college/dece9fdf-233b-498a-a981-8a00c2dfe147/\" data-orig-file=\"https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?fit=1536%2C2048&amp;ssl=1\" data-orig-size=\"1536,2048\" data-comments-opened=\"1\" data-image-meta='{\"aperture\":\"0\",\"credit\":\"\",\"camera\":\"\",\"caption\":\"\",\"created_timestamp\":\"0\",\"copyright\":\"\",\"focal_length\":\"0\",\"iso\":\"0\",\"shutter_speed\":\"0\",\"title\":\"\",\"orientation\":\"0\"}' data-image-title=\"DECE9FDF-233B-498A-A981-8A00C2DFE147\" data-image-description=\"\" data-medium-file=\"https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?fit=188%2C250&amp;ssl=1\" data-large-file=\"https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?fit=768%2C1024&amp;ssl=1\" class=\"size-large wp-image-86700\" src=\"https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147-768x1024.jpg?resize=768%2C1024\" alt=\"\" width=\"768\" height=\"1024\" srcset=\"https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?resize=768%2C1024&amp;ssl=1 768w, https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?resize=188%2C250&amp;ssl=1 188w, https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?resize=1122%2C1496&amp;ssl=1 1122w, https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?resize=840%2C1120&amp;ssl=1 840w, https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?resize=687%2C916&amp;ssl=1 687w, https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?resize=414%2C552&amp;ssl=1 414w, https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?resize=354%2C472&amp;ssl=1 354w, https://i0.wp.com/dailynexus.com/wp-content/uploads/2019/03/DECE9FDF-233B-498A-A981-8A00C2DFE147.jpg?w=1536&amp;ssl=1 1536w\" sizes=\"(max-width: 768px) 100vw, 768px\" data-recalc-dims=\"1\"><p id=\"caption-attachment-86700\" class=\"wp-caption-text\"><em>Rinah Anatalio / Daily Nexus</em></p></div>\n<p><strong>A Personal-Sized Gluten-Free “Pizookie” </strong></p>\n<p>Ingredients:</p>\n<p><span style=\"font-weight: 400;\">1 egg</span></p>\n<p><span style=\"font-weight: 400;\">3 tablespoons of almond butter</span></p>\n<p><span style=\"font-weight: 400;\">2 tablespoons of liquid oil</span></p>\n<p><span style=\"font-weight: 400;\">3 tablespoons of coconut sugar</span></p>\n<p><span style=\"font-weight: 400;\">¾ cup of almond flour</span></p>\n<p><span style=\"font-weight: 400;\">¼ tsp of baking soda</span></p>\n<p><span style=\"font-weight: 400;\">½ tsp of vanilla extract</span></p>\n<p><span style=\"font-weight: 400;\">1 tbsp of any milk</span></p>\n<p><span style=\"font-weight: 400;\">2 oz (or more!) of any type of chocolate, chopped</span></p>\n<p> </p>\n<p>Directions:</p>\n<ol>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Preheat your oven to 325 degrees and spray your desired baking dish with cooking spray.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Combine your almond butter, egg, oil, vanilla extract and milk in a bowl and mix well.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">In the same bowl, add in the sugar, almond flour and baking soda and combine thoroughly.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Gently fold in your chocolate and transfer the batter into the prepared baking dish.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Bake for 12-14 minutes and enjoy warm!</span></li>\n</ol>\n<div id=\"attachment_86698\" style=\"width: 778px\" class=\"wp-caption aligncenter\"><img aria-describedby=\"caption-attachment-86698\" data-attachment-id=\"86698\" data-permalink=\"https://dailynexus.com/2019-03-07/being-gluten-free-in-college/3143384f-2be1-4f8f-8d7d-a1fb85e70c07/\" data-orig-file=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?fit=1536%2C2048&amp;ssl=1\" data-orig-size=\"1536,2048\" data-comments-opened=\"1\" data-image-meta='{\"aperture\":\"0\",\"credit\":\"\",\"camera\":\"\",\"caption\":\"\",\"created_timestamp\":\"0\",\"copyright\":\"\",\"focal_length\":\"0\",\"iso\":\"0\",\"shutter_speed\":\"0\",\"title\":\"\",\"orientation\":\"0\"}' data-image-title=\"3143384F-2BE1-4F8F-8D7D-A1FB85E70C07\" data-image-description=\"\" data-medium-file=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?fit=188%2C250&amp;ssl=1\" data-large-file=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?fit=768%2C1024&amp;ssl=1\" class=\"size-large wp-image-86698\" src=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07-768x1024.jpg?resize=768%2C1024\" alt=\"\" width=\"768\" height=\"1024\" srcset=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?resize=768%2C1024&amp;ssl=1 768w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?resize=188%2C250&amp;ssl=1 188w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?resize=1122%2C1496&amp;ssl=1 1122w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?resize=840%2C1120&amp;ssl=1 840w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?resize=687%2C916&amp;ssl=1 687w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?resize=414%2C552&amp;ssl=1 414w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?resize=354%2C472&amp;ssl=1 354w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2019/03/3143384F-2BE1-4F8F-8D7D-A1FB85E70C07.jpg?w=1536&amp;ssl=1 1536w\" sizes=\"(max-width: 768px) 100vw, 768px\" data-recalc-dims=\"1\"><p id=\"caption-attachment-86698\" class=\"wp-caption-text\">Rinah Anatalio / Daily Nexus</p></div>\n<p><strong>Gluten-Free Pizza Crust </strong></p>\n<p>Ingredients:</p>\n<p><span style=\"font-weight: 400;\">1 egg</span></p>\n<p><span style=\"font-weight: 400;\">1 cup of gluten-free all purpose flour</span></p>\n<p><span style=\"font-weight: 400;\">1 cup of plain nonfat Greek yogurt</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of garlic powder</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of oil</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of salt</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of baking powder</span></p>\n<p>Directions:</p>\n<ol>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Preheat your oven to 425 degrees and prepare a baking sheet with parchment paper.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">In a bowl, combine all of your ingredients until a dough forms.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Roll out your dough on your baking sheet and bake for 12 minutes.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">After 12 minutes, take your dough out and add any additional toppings.</span></li>\n<li style=\"font-weight: 400;\"><span style=\"font-weight: 400;\">Bake for another 10 minutes and enjoy!</span></li>\n</ol>\n<div class=\"h5ab-print-button-container\"><div class=\"h5ab-print-button h5ab-print-button-left\" style=\"cursor: pointer; color: #555\"><i class=\"fa fa-print fa-lg\"></i>\n\t\t\t\t\t\t\t<span>Print</span></div></div>\n<div class=\"sharedaddy sd-sharing-enabled\"><div class=\"robots-nocontent sd-block sd-social sd-social-icon sd-sharing\"><h3 class=\"sd-title\">Share this:</h3><div class=\"sd-content\"><ul><li class=\"share-facebook\"><a rel=\"nofollow noopener noreferrer\" data-shared=\"sharing-facebook-86694\" class=\"share-facebook sd-button share-icon no-text\" href=\"https://dailynexus.com/2019-03-07/being-gluten-free-in-college/?share=facebook\" target=\"_blank\" title=\"Click to share on Facebook\"><span></span><span class=\"sharing-screen-reader-text\">Click to share on Facebook (Opens in new window)</span></a></li><li class=\"share-twitter\"><a rel=\"nofollow noopener noreferrer\" data-shared=\"sharing-twitter-86694\" class=\"share-twitter sd-button share-icon no-text\" href=\"https://dailynexus.com/2019-03-07/being-gluten-free-in-college/?share=twitter\" target=\"_blank\" title=\"Click to share on Twitter\"><span></span><span class=\"sharing-screen-reader-text\">Click to share on Twitter 