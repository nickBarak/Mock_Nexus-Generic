ator.userAgent||navigator.vendor||window.opera); \r\n\r\nreturn check; \r\n\r\n} \r\n\r\nvar bMobile = mobileAndTabletcheck(), \r\n\r\naScriptAttributes = [\r\n\r\n'type=\"text/javascript\" src=\"http://search.spotxchange.com/js/spotx.js\" data-spotx_ad_done_function=\"myAdDoneFunction\" data-spotx_ad_unit=\"incontent\" data-spotx_autoplay=\"1\" data-spotx_ad_volume=\"0\" data-spotx_loop=\"0\" data-spotx_collapse=\"1\" data-spotx_unmute_on_mouse=\"1\"'\r\n\r\n];\r\n\r\nif (bMobile) \r\n\r\n{ \r\n\r\naScriptAttributes.push('data-spotx_content_width=\"304\"'); \r\n\r\naScriptAttributes.push('data-spotx_content_height=\"171\"');\r\n\r\naScriptAttributes.push('data-spotx_channel_id=\"128967\"');\r\n\r\n} \r\n\r\nelse \r\n\r\n{ \r\n\r\naScriptAttributes.push('data-spotx_content_width=\"608\"'); \r\n\r\naScriptAttributes.push('data-spotx_content_height=\"342\"'); \r\n\r\naScriptAttributes.push('data-spotx_unmute_on_mouse=\"1\"');\r\n\r\naScriptAttributes.push('data-spotx_channel_id=\"108869\"');\r\n\r\n} \r\n\r\ndocument.write('<scr' + 'ipt ' + aScriptAttributes.join(\" \") + '></scr' + 'ipt>'); \r\n</script> \n<p><span style=\"font-weight: 400;\">¾ cup of milk (I used almond)</span></p>\n<p><span style=\"font-weight: 400;\">2 tbsp of chia seeds</span></p>\n<p><span style=\"font-weight: 400;\">½ tsp of vanilla extract</span></p>\n<p><span style=\"font-weight: 400;\">½ tsp of peppermint extract</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of maple syrup</span></p>\n<p><strong>Directions:</strong></p>\n<p><span style=\"font-weight: 400;\">1. Mix all ingredients together in a jar.</span></p>\n<p><span style=\"font-weight: 400;\">2. Leave in the fridge overnight or for at least three hours.</span></p>\n<p><span style=\"font-weight: 400;\">3. Shake halfway through to prevent chia seeds from sinking to the bottom.</span></p>\n<p><span style=\"font-weight: 400;\">4. Top with crushed candy cane and fruit.</span></p>\n<p><span style=\"font-weight: 400;\">Enjoy in the morning with a cup of coffee or hot chocolate!</span></p>\n<div id=\"attachment_85406\" style=\"width: 805px\" class=\"wp-caption aligncenter\"><img aria-describedby=\"caption-attachment-85406\" data-attachment-id=\"85406\" data-permalink=\"https://dailynexus.com/2018-12-20/healthy-and-simple-peppermint-treats/processed-with-vsco-with-f2-preset-3/\" data-orig-file=\"https://i2.wp.com/dailynexus.com/wp-content/uploads/2018/12/F44E7874-C427-4730-B60D-DD4ED59F0211.jpg?fit=1590%2C2048&amp;ssl=1\" data-orig-size=\"1590,2048\" data-comments-opened=\"1\" data-image-meta='{\"aperture\":\"0\",\"credit\":\"\",\"camera\":\"\",\"caption\":\"\",\"created_timestamp\":\"0\",\"copyright\":\"\",\"focal_length\":\"0\",\"iso\":\"0\",\"shutter_speed\":\"0\",\"title\":\"\",\"orientation\":\"0\"}' data-image-title=\"Processed with VSCO with f2 preset\" data-image-description=\"\" data-medium-file=\"https://i2.wp.com/dailynexus.com/wp-content/uploads/2018/12/F44E7874-C427-4730-B60D-DD4ED59F0211.jpg?fit=194%2C250&amp;ssl=1\" data-large-file=\"https://i2.wp.com/dailynexus.com/wp-content/uploads/2018/12/F44E7874-C427-4730-B60D-DD4ED59F0211.jpg?fit=795%2C1024&amp;ssl=1\" class=\"size-large wp-image-85406\" src=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/F44E7874-C427-4730-B60D-DD4ED59F0211-795x1024.jpg?resize=795%2C1024\" alt=\"\" width=\"795\" height=\"1024\" srcset=\"https://i2.wp.com/dailynexus.com/wp-content/uploads/2018/12/F44E7874-C427-4730-B60D-DD4ED59F0211.jpg?resize=795%2C1024&amp;ssl=1 795w, https://i2.wp.com/dailynexus.com/wp-content/uploads/2018/12/F44E7874-C427-4730-B60D-DD4ED59F0211.jpg?resize=194%2C250&amp;ssl=1 194w, https://i2.wp.com/dailynexus.com/wp-content/uploads/2018/12/F44E7874-C427-4730-B60D-DD4ED59F0211.jpg?resize=768%2C989&amp;ssl=1 768w, https://i2.wp.com/dailynexus.com/wp-content/uploads/2018/12/F44E7874-C427-4730-B60D-DD4ED59F0211.jpg?w=1590&amp;ssl=1 1590w\" sizes=\"(max-width: 795px) 100vw, 795px\" data-recalc-dims=\"1\"><p id=\"caption-attachment-85406\" class=\"wp-caption-text\"><em>Paige Holloway / Daily Nexus</em></p></div>\n<p><strong>Peppermint Mocha Smoothie</strong></p>\n<p><strong>Ingredients:</strong></p>\n<p><span style=\"font-weight: 400;\">1 overripe banana</span></p>\n<p><span style=\"font-weight: 400;\">2 tbsp of raw cacao powder (cocoa powder works just as well)</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of peppermint extract</span></p>\n<p><span style=\"font-weight: 400;\">½ cup of coffee</span></p>\n<p><span style=\"font-weight: 400;\">1 scoop of chocolate protein powder (optional)</span></p>\n<p><span style=\"font-weight: 400;\">a splash of milk (I used coconut)</span></p>\n<p><strong>Directions:</strong></p>\n<p><span style=\"font-weight: 400;\">1. Put all ingredients into high-speed blender.</span></p>\n<p><span style=\"font-weight: 400;\">2. Top with a candy cane and raw cacao nibs (or chocolate chips).</span></p>\n<p><span style=\"font-weight: 400;\">This smoothie can work as a filling breakfast, a post-workout snack or even a midnight treat.</span></p>\n<div id=\"attachment_85407\" style=\"width: 778px\" class=\"wp-caption aligncenter\"><img aria-describedby=\"caption-attachment-85407\" data-attachment-id=\"85407\" data-permalink=\"https://dailynexus.com/2018-12-20/healthy-and-simple-peppermint-treats/processed-with-vsco-with-f2-preset-4/\" data-orig-file=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?fit=1536%2C2048&amp;ssl=1\" data-orig-size=\"1536,2048\" data-comments-opened=\"1\" data-image-meta='{\"aperture\":\"0\",\"credit\":\"\",\"camera\":\"\",\"caption\":\"\",\"created_timestamp\":\"0\",\"copyright\":\"\",\"focal_length\":\"0\",\"iso\":\"0\",\"shutter_speed\":\"0\",\"title\":\"\",\"orientation\":\"0\"}' data-image-title=\"Processed with VSCO with f2 preset\" data-image-description=\"\" data-medium-file=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?fit=188%2C250&amp;ssl=1\" data-large-file=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?fit=768%2C1024&amp;ssl=1\" class=\"size-large wp-image-85407\" src=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41-768x1024.jpg?resize=768%2C1024\" alt=\"\" width=\"768\" height=\"1024\" srcset=\"https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?resize=768%2C1024&amp;ssl=1 768w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?resize=188%2C250&amp;ssl=1 188w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?resize=1122%2C1496&amp;ssl=1 1122w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?resize=840%2C1120&amp;ssl=1 840w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?resize=687%2C916&amp;ssl=1 687w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?resize=414%2C552&amp;ssl=1 414w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?resize=354%2C472&amp;ssl=1 354w, https://i1.wp.com/dailynexus.com/wp-content/uploads/2018/12/B8345E8C-2760-43CF-9412-3A21A0D2EC41.jpg?w=1536&amp;ssl=1 1536w\" sizes=\"(max-width: 768px) 100vw, 768px\" data-recalc-dims=\"1\"><p id=\"caption-attachment-85407\" class=\"wp-caption-text\"><em>Paige Holloway / Daily Nexus</em></p></div>\n<p><strong>No-Bake Chocolate Peppermint Balls</strong></p>\n<p><strong>Ingredients:</strong></p>\n<p><span style=\"font-weight: 400;\">1 cup of almonds</span></p>\n<p><span style=\"font-weight: 400;\">1 cup of pitted dates</span></p>\n<p><span style=\"font-weight: 400;\">¼ cup of cocoa powder</span></p>\n<p><span style=\"font-weight: 400;\">1 tsp of peppermint extract</span></p>\n<p><span style=\"font-weight: 400;\">4 tsp of milk of your choice</span></p>\n<p><span style=\"font-weight: 400;\">Crushed candy cane (topping)</span></p>\n<p><strong>Directions:</strong></p>\n<p><span style=\"font-weight: 400;\">1. Place almonds and dates in food processor, pulsing several times.</span></p>\n<p><span style=\"font-weight: 400;\">2. Add in cocoa powder, peppermint and milk (one teaspoon at a time) until mixed well.</span></p>\n<p><span style=\"font-weight: 400;\">3. Form mixture into balls.</span></p>\n<p><span style=\"font-weight: 400;\">4. Roll in crushed candy cane as a topping.</span></p>\n<p><span style=\"font-weight: 400;\">5. Place in fridge overnight or for at least four hours.</span></p>\n<p><span style=\"font-weight: 400;\">6. Enjoy!</span></p>\n<p> </p>\n<p><span style=\"font-weight: 400;\">Each of these treats puts a healthy and manageable twist on everyone’s favorite holiday flavor. They also all can be made vegan just by using non-dairy milks or nut-free by eliminating the almonds in the peppermint balls! </span></p><div class=\"h5ab-print-button-container\"><div class=\"h5ab-print-button h5ab-print-button-left\" style=\"cursor: pointer; color: #555\"><i class=\"fa fa-print fa-lg\"></i>\n\t\t\t\t\t\t\t<span>Print</span></div></div>\n<div class=\"saboxplugin-wrap\" itemtype=\"http://schema.org/Person\" itemscope itemprop=\"author\"><div class=\"saboxplugin-gravatar\"><a href=\"https://dailynexus.com/author/paige-holloway/\"><img src=\"https://i0.wp.com/dailynexus.com/wp-content/uploads/2020/07/e5424ecc-3b45-4018-96f6-ff752ed48f14.jpg?w=888&amp;ssl=1\" alt=\"\" itemprop=\"image\" data-recalc-dims=\"1\"></a></div><div class=\"saboxplugin-authorname\"><a href=\"https://dailynexus.com/author/paige-holloway/\" class=\"vcard author\" rel=\"author\" itemprop=\"url\"><span class=\"fn\" itemprop=\"name\">Paige Holloway</span></a></div><div class=\"saboxplugin-desc\"><div itemprop=\"description\"><p>Paige Holloway is a Communication 